<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.attendance.pro.dao.LangaugeMasterDao">

<sql id="columns">
window_id, master_name, lang, langValue
</sql>

<resultMap type="com.attendance.pro.dto.LangaugeMasterDto" id="LangaugeMasterDto">
	<result column="window_id" property="windowId" />
	<result column="master_name" property="masterName" />
	<result column="lang" property="lang" />
	<result column="langValue" property="langValue" />
</resultMap>

<select id="getAllLangs" resultMap="LangaugeMasterDto">
	SELECT <include refid="columns"></include>
	FROM
		LANGAUGE_MASTER 
	WHERE
		DEL_FLG = "0"
</select>


</mapper>